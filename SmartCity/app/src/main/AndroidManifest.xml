<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aaronhu.maptest" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".HandleData">


        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ParkingLotActivity"
            android:label="parking lot" >
        </activity>
        <activity
            android:name=".ApplyLockerActivity"
            android:label="apply Locker" >
        </activity>
        <activity
            android:name=".WifiActivity"
            android:label="wifi" >
        </activity>
        <activity
            android:name=".TimePickerActivity"
            android:label="time picker" >
        </activity>

        <uses-permission android:name="android.permission.INTERNET" />
    </application>

</manifest>
